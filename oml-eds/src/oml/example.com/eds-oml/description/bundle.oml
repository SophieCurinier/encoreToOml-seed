@dc:description "A description bundle for closed-world reasoning about an electronic datasheet"
description bundle <http://example.com/eds-oml/description/bundle#> as ^bundle {
	
	uses <http://purl.org/dc/elements/1.1/> as dc
    
    // The description bundle "uses" the vocabulary bundle world closure axioms
    uses <http://example.com/eds-oml/vocabulary/datasheet-bundle#>


    // The description bundle "uses" the lamp description
	includes <http://example.com/eds-oml/description/lamp#>
}